package com.egame.beans;

/**
 * 描述:联系人列表实体类
 * 
 * @author LiuHan
 * @version 1.0 create date :Sat Dec 31 2011
 */
public class ContactsBean {
	/** 联系人姓名 */
	private String mContactsName;
	/** 联电话 */
	private String mContactsPhone;
	/** 当前是否被选中 */
	private boolean mIsSelect;

	/**
	 * 构造器
	 */
	public ContactsBean() {
	}

	public String getmContactsName() {
		return mContactsName;
	}

	public void setmContactsName(String mContactsName) {
		this.mContactsName = mContactsName;
	}

	public String getmContactsPhone() {
		return mContactsPhone;
	}

	public void setmContactsPhone(String mContactsPhone) {
		this.mContactsPhone = mContactsPhone;
	}

	public boolean ismIsSelect() {
		return mIsSelect;
	}

	public void setmIsSelect(boolean mIsSelect) {
		this.mIsSelect = mIsSelect;
	}

}
